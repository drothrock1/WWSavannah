                        <label opaque="NO" clipsSubviews="YES" userInteractionEnabled="NO" contentMode="left" text="Recovering Civil War remains" lineBreakMode="tailTruncation" minimumFontSize="10" id="DOJ-rv-akL">
                            <rect key="frame" x="30" y="2663" width="280" height="21"/>
                            <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMaxY="YES"/>
                            <fontDescription key="fontDescription" name="Georgia-Italic" family="Georgia" pointSize="14"/>
